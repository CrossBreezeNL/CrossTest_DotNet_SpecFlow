<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>CrossBreeze.CrossTest</id>
    <version>1.0.1.0</version>
    <title>CrossTest</title>
    <authors>Harmen Wessels, Jacob Siemaszko, Willem Otten</authors>
    <owners>CrossBreeze</owners>
    <license type="file">LICENSE.txt</license>
    <projectUrl>http://www.x-test.nl</projectUrl>
    <PackageIcon>http://x-test.nl/logo/x-test-icon.png</PackageIcon>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>An open-source data solution testing library (using SpecFlow).</description>
    <releaseNotes>Enable setting command timeout on databaseserver in config. 
    Please note that with this version the crosstest section in the app.config needs to refer to CrossBreeze.CrossTest.SpecFlow.Core instead of CrossBreeze.CrossTest.SpecFlow. This should happen automatically by updating to this version of Crosstest</releaseNotes>
    <copyright>Copyright CrossBreeze 2020</copyright>
    <tags>Test Database DWH BI SQL T-SQL SSIS SSAS</tags>
    <dependencies>
      <dependency id="SpecFlow" version="[2.4.0,2.4.1]" />
      <dependency id="Microsoft.AnalysisServices.AdomdClient.retail.amd64" version="15.12.1" />
      <dependency id="MSTest.TestFramework" version="1.4.0" />
      <!-- Not strictly a depedency for this NuGet package, but the user will need it to execute the tests. -->
      <dependency id="MSTest.TestAdapter" version="1.4.0" />
      <!-- In the SMO library the STrace namespace exists and is needed for SSIS package execution. -->
      <dependency id="Microsoft.SqlServer.SqlManagementObjects" version="140.17283.0" />
    </dependencies>
    <!-- Add the framework assemblies that should be referenced in the C# project. -->
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System" />
    </frameworkAssemblies>
    <!-- The content files element is used by PackageReference in projects. -->
    <contentFiles>
      <files include=".\NuGetFiles\app.config.transform" buildAction="Content" />
    </contentFiles>
  </metadata>
  <files>
    <file src=".\NuGetFiles\LICENSE.txt" target="" />
    <file src=".\CrossBreeze_CrossDoc\bin\Release\CrossBreeze.CrossDoc.dll" target="lib\net45" />
    <file src=".\CrossBreeze_CrossTest_Database\bin\Release\CrossBreeze.CrossTest.Database.dll" target="lib\net45" />
    <file src=".\CrossBreeze_CrossTest_Process_SSIS\bin\Release\CrossBreeze.CrossTest.Process.Ssis.dll" target="lib\net45" />
    <file src=".\CrossBreeze_CrossTest_SpecFlow_Core\bin\Release\CrossBreeze.CrossTest.SpecFlow.Core.dll" target="lib\net45" />
    <file src=".\CrossBreeze_CrossTest_SpecFlow_Steps\Generated\CrossBreeze.CrossTest.SpecFlow.dll" target="lib\net45" />
    <!-- The content file definition when using packages.config in projects. -->
    <file src=".\NuGetFiles\app.config.transform" target="content" />
  </files>
</package>